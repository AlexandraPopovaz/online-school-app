{
  "name": "online-school-app",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "scripts": {
    "build": "rimraf build && tsc -p .",
    "dev": "NODE_ENV=dev nodemon src/app.ts",
    "prod": "NODE_ENV=production node build/app.js",
    "dc:dev": "docker-compose --env-file ./env/.env.dev up --build",
    "dc:prod": "docker-compose -f docker-compose.prod.yml --env-file ./env/.env.production up --build",
    "dc:down": "docker-compose down",
    "db:migrate:dev": "NODE_ENV=dev npx sequelize-cli db:migrate",
    "db:undo:dev": "NODE_ENV=dev npx sequelize-cli db:migrate:undo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.14.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0"
  }
