{
  "name": "online-school-app",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "scripts": {
    "build": "rimraf build && tsc -p .",
    "test": "jest --watch",
    "test:nowatch": "jest",
    "dev": "NODE_ENV=dev nodemon src/app.ts --ignore 'test/*'",
    "prod": "NODE_ENV=production node build/app.js",
    "dc:dev": "docker-compose --env-file ./env/.env.dev up --build",
    "dc:prod": "docker-compose -f docker-compose.prod.yml --env-file ./env/.env.production up --build",
    "dc:down": "docker-compose down",
    "db:migrate:dev": "NODE_ENV=dev npx sequelize-cli db:migrate",
    "db:undo:dev": "NODE_ENV=dev npx sequelize-cli db:migrate:undo",
    "db:seed:all": "NODE_ENV=dev npx sequelize-cli db:seed:all",
    "db:seed:undo": "NODE_ENV=dev npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@types/axios": "0.14.0",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.13",
    "@types/express-validator": "3.0.0",
    "@types/faker": "6.6.9",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.178",
    "@types/passport": "1.0.7",
    "@types/passport-http": "0.3.9",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "axios": "0.26.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "rimraf": "3.0.2",
    "sequelize-cli": "6.4.1",
    "ts-jest": "^27.1.3",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.1",
    "dotenv": "14.3.2",
    "express": "4.17.2",
    "express-validator": "6.14.0",
    "lodash": "4.17.21",
    "passport": "0.5.2",
    "passport-headerapikey": "1.2.2",
    "passport-http": "0.3.0",
    "pg": "8.7.1",
    "pg-hstore": "2.3.4",
    "sequelize": "6.14.1",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "4.3.0"
  }
}
